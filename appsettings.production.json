{
    "DeviceId": "",
    "EnableMqtt": true,
    "EnableSerial": true,
    "Mqtt": {
        "BrokerHost": "${MQTT_BROKER_HOST:localhost}",
        "BrokerPort": "${MQTT_BROKER_PORT:1883}",
        "Username": "${MQTT_USERNAME}",
        "Password": "${MQTT_PASSWORD}",
        "ClientId": "${MQTT_CLIENT_ID}",
        "UseTls": "${MQTT_USE_TLS:false}"
    },
    "Serial": {
        "PortName": "${SERIAL_PORT:COM8}",
        "BaudRate": "${SERIAL_BAUD_RATE:115200}"
    }
}